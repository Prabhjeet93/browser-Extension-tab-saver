{
  "manifest_version": 2,
  "name": "Tab Saver",
  "version": "1.0",
  "description": "Save current tabs into folders for temporary use.",
  "permissions": ["tabs", "storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}